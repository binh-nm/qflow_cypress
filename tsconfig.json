{
  "baseUrl": "./cypress",
  "include": [
    "./**/*.ts",
    "integration/*.ts",
    "support/*.ts",
    "../node_modules/cypress"
  ],
  "exclude": [],
  "compilerOptions": {
    "types": ["cypress", "node"],
    "lib": ["es2017", "dom"],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react"
  }
}
